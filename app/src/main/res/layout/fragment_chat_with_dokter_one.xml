<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="chatWithDokterOneVM" type="com.dessapplication.app.modules.chatwithdokterone.data.viewmodel.ChatWithDokterOneVM"/>
    </data>

    <ScrollView
        android:id="@+id/scrollViewScrollview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:clipToPadding="false"
        android:fillViewport="true">

        <LinearLayout
            android:id="@+id/linearRowairplane"
            style="@style/groupStylegray_900_4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <LinearLayout
                android:id="@+id/linearRowellipsesixteen"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_14pxh"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_marginBottom="@dimen/_16pxv"
                android:layout_weight="0.93"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/imageEllipseSixteen"
                    circular="@{true}"
                    imageRes="@{@drawable/img_rectangle21}"
                    android:layout_width="@dimen/_38pxh"
                    android:layout_height="@dimen/_37pxh"
                    android:scaleType="centerCrop"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_rectangle21" />

                <LinearLayout
                    android:id="@+id/linearColumnlanguage"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_17pxh"
                    android:layout_marginTop="@dimen/_6pxv"
                    android:layout_marginBottom="@dimen/_4pxv"
                    android:layout_weight="0.72"
                    android:gravity="start"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/txtLanguage"
                        style="@style/txtPlusJakartaSansromanbold11"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_2pxh"
                        android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:singleLine="true"
                        android:text="@{chatWithDokterOneVM.chatWithDokterOneModel.txtLanguage}"
                        tools:text="@string/msg_dr_boyke_sudra2" />

                    <LinearLayout
                        android:id="@+id/linearRowfortyeight"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:gravity="start"
                        android:orientation="horizontal">

                        <FrameLayout
                            android:id="@+id/frameStackfortyeight"
                            android:layout_width="@dimen/_19pxh"
                            android:layout_height="@dimen/_12pxv">

                            <TextView
                                android:id="@+id/txtFortyEight"
                                style="@style/txtPlusJakartaSansromansemibold5"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="end"
                                android:layout_marginStart="@dimen/_10pxh"
                                android:layout_marginTop="@dimen/_2pxv"
                                android:layout_marginBottom="@dimen/_2pxv"
                                android:ellipsize="end"
                                android:singleLine="true"
                                android:text="@{chatWithDokterOneVM.chatWithDokterOneModel.txtFortyEight}"
                                tools:text="@string/lbl_4_8" />

                            <ImageView
                                android:id="@+id/imageStarTwo"
                                android:layout_width="@dimen/_11pxh"
                                android:layout_height="@dimen/_12pxh"
                                android:layout_gravity="start"
                                android:layout_marginEnd="@dimen/_10pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_star2_12x11"
                                tools:ignore="ContentDescription"
                                tools:src="@drawable/img_star2_12x11" />
                        </FrameLayout>

                        <View
                            android:id="@+id/viewEllipseThree"
                            android:layout_width="@dimen/_2pxh"
                            android:layout_height="@dimen/_2pxh"
                            android:layout_marginStart="@dimen/_5pxh"
                            android:layout_marginTop="@dimen/_5pxv"
                            android:layout_marginBottom="@dimen/_5pxv"
                            android:background="@drawable/rectangle_bg_bluegray_100_radius_1" />

                        <TextView
                            android:id="@+id/txtDokterUmum"
                            style="@style/txtPlusJakartaSansromanmedium6"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_5pxh"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_marginBottom="@dimen/_2pxv"
                            android:ellipsize="end"
                            android:singleLine="true"
                            android:text="@{chatWithDokterOneVM.chatWithDokterOneModel.txtDokterUmum}"
                            tools:text="@string/lbl_dokter_umum" />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

            <ImageView
                android:id="@+id/imageAirplane"
                android:layout_width="@dimen/_12pxh"
                android:layout_height="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_5pxh"
                android:layout_marginTop="@dimen/_23pxv"
                android:layout_marginEnd="@dimen/_132pxh"
                android:layout_marginBottom="@dimen/_36pxv"
                android:scaleType="fitXY"
                android:src="@drawable/img_airplane"
                tools:ignore="ContentDescription"
                tools:src="@drawable/img_airplane" />
        </LinearLayout>
    </ScrollView>
</layout>
