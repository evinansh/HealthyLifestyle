<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="pertanyaanTesKesehatanMentalVM" type="com.dessapplication.app.modules.pertanyaanteskesehatanmental.data.viewmodel.PertanyaanTesKesehatanMentalVM"/>
    </data>
    <FrameLayout android:id="@+id/framePertanyaantes"
        android:layout_height="match_parent"
        android:layout_marginBottom="@dimen/_7pxv"
        android:layout_marginEnd="@dimen/_8pxh"
        android:layout_marginStart="@dimen/_11pxh"
        android:layout_marginTop="@dimen/_29pxv"
        android:layout_width="match_parent" style="@style/groupStyleblack_900">
        <LinearLayout android:id="@+id/linearColumnarrowleft"
            android:layout_gravity="top|center"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_10pxv"
            android:layout_marginEnd="@dimen/_9pxh"
            android:layout_marginStart="@dimen/_9pxh"
            android:layout_width="match_parent" android:orientation="vertical">
            <androidx.appcompat.widget.Toolbar
                android:gravity="center_horizontal"
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_2pxh"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:id="@+id/linearRowgroupfourteen"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <LinearLayout android:id="@+id/linearRowarrowleft"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.97"
                        android:layout_width="0dp" android:orientation="horizontal">
                        <FrameLayout
                            android:id="@+id/frameStackarrowleft"
                            android:layout_height="@dimen/_22pxv"
                            android:layout_width="@dimen/_24pxh" style="@style/groupStylecornerRadius">
                            <ImageView android:id="@+id/imageArrowleft"
                                android:layout_gravity="start"
                                android:layout_height="@dimen/_12pxh"
                                android:layout_marginBottom="@dimen/_4pxv"
                                android:layout_marginEnd="@dimen/_10pxh"
                                android:layout_marginStart="@dimen/_7pxh"
                                android:layout_marginTop="@dimen/_5pxv"
                                android:layout_width="@dimen/_7pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_arrowleft"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                        </FrameLayout>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtHealthQuiz"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_2pxv"
                            android:layout_marginStart="@dimen/_98pxh"
                            android:layout_marginTop="@dimen/_2pxv"
                            android:layout_weight="0.75"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{pertanyaanTesKesehatanMentalVM.pertanyaanTesKesehatanMentalModel.txtHealthQuiz}"
                            style="@style/txtPlusJakartaSansromanbold13" tools:text="@string/lbl_health_quiz"/>
                    </LinearLayout>
                    <ImageView android:id="@+id/imageGroupFourteen"
                        android:layout_height="@dimen/_19pxh"
                        android:layout_marginBottom="@dimen/_2pxv"
                        android:layout_marginStart="@dimen/_122pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_width="@dimen/_5pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_group14"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_group14"/>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
            <ImageView android:gravity="center_horizontal"
                android:id="@+id/imageRectangleEightyOne"
                android:layout_height="@dimen/_167pxh"
                android:layout_marginEnd="@dimen/_4pxh"
                android:layout_marginTop="@dimen/_18pxv"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                cornerRadius="@{@dimen/_25pxh}"
                imageRes="@{@drawable/img_rectangle81}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle81"/>
            <LinearLayout android:id="@+id/linearRowrectangleeightysix"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_26pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <View
                    android:background="@drawable/rectangle_bg_indigo_a701_radius_2_5"
                    android:id="@+id/viewRectangleEightySix"
                    android:layout_height="@dimen/_5pxv" android:layout_width="@dimen/_27pxh"/>
                <View
                    android:background="@drawable/rectangle_bg_bluegray_100_radius_2_5"
                    android:id="@+id/viewRectangleEightySeven"
                    android:layout_height="@dimen/_5pxv"
                    android:layout_marginStart="@dimen/_7pxh" android:layout_width="@dimen/_27pxh"/>
                <View
                    android:background="@drawable/rectangle_bg_bluegray_100_radius_2_5"
                    android:id="@+id/viewRectangleEightyEight"
                    android:layout_height="@dimen/_5pxv"
                    android:layout_marginStart="@dimen/_7pxh" android:layout_width="@dimen/_27pxh"/>
                <View
                    android:background="@drawable/rectangle_bg_bluegray_100_radius_2_5"
                    android:id="@+id/viewRectangleEightyNine"
                    android:layout_height="@dimen/_5pxv"
                    android:layout_marginStart="@dimen/_7pxh" android:layout_width="@dimen/_27pxh"/>
                <View
                    android:background="@drawable/rectangle_bg_bluegray_100_radius_2_5"
                    android:id="@+id/viewRectangleNinety"
                    android:layout_height="@dimen/_5pxv"
                    android:layout_marginStart="@dimen/_7pxh" android:layout_width="@dimen/_27pxh"/>
                <View
                    android:background="@drawable/rectangle_bg_bluegray_100_radius_2_5"
                    android:id="@+id/viewRectangleNinetyOne"
                    android:layout_height="@dimen/_5pxv"
                    android:layout_marginStart="@dimen/_7pxh" android:layout_width="@dimen/_27pxh"/>
                <View
                    android:background="@drawable/rectangle_bg_bluegray_100_radius_2_5"
                    android:id="@+id/viewRectangleNinetyTwo"
                    android:layout_height="@dimen/_5pxv"
                    android:layout_marginStart="@dimen/_7pxh" android:layout_width="@dimen/_27pxh"/>
                <View
                    android:background="@drawable/rectangle_bg_bluegray_100_radius_2_5"
                    android:id="@+id/viewRectangleNinetyThree"
                    android:layout_height="@dimen/_5pxv"
                    android:layout_marginStart="@dimen/_7pxh" android:layout_width="@dimen/_27pxh"/>
                <View
                    android:background="@drawable/rectangle_bg_bluegray_100_radius_2_5"
                    android:id="@+id/viewRectangleNinetyFour"
                    android:layout_height="@dimen/_5pxv"
                    android:layout_marginStart="@dimen/_7pxh" android:layout_width="@dimen/_27pxh"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumnpertanyaan"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_4pxh"
                android:layout_marginTop="@dimen/_8pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylegray_900_3">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtPertanyaan"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_26pxh"
                    android:layout_marginStart="@dimen/_26pxh"
                    android:layout_marginTop="@dimen/_13pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{pertanyaanTesKesehatanMentalVM.pertanyaanTesKesehatanMentalModel.txtPertanyaan}"
                    style="@style/txtPlusJakartaSansromanbold15" tools:text="@string/lbl_pertanyaan"/>
                <TextView android:id="@+id/txtTime"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_26pxh"
                    android:layout_marginStart="@dimen/_26pxh"
                    android:layout_marginTop="@dimen/_18pxv"
                    android:layout_width="match_parent"
                    android:text="@{pertanyaanTesKesehatanMentalVM.pertanyaanTesKesehatanMentalModel.txtTime}"
                    style="@style/txtPlusJakartaSansromanmedium13" tools:text="@string/msg_dalam_4_minggu"/>
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnTidakPernah"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_25pxh"
                    android:layout_marginStart="@dimen/_26pxh"
                    android:layout_marginTop="@dimen/_28pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_12pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_12pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_tidak_pernah"
                    android:textAllCaps="false"
                    style="@style/btnSolidRounded_2" tools:text="@string/lbl_tidak_pernah"/>
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnJarang"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_25pxh"
                    android:layout_marginStart="@dimen/_26pxh"
                    android:layout_marginTop="@dimen/_9pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_12pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_12pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_jarang"
                    android:textAllCaps="false"
                    style="@style/btnSolidRounded_2" tools:text="@string/lbl_jarang"/>
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnKadangKadangOne"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_23pxh"
                    android:layout_marginStart="@dimen/_26pxh"
                    android:layout_marginTop="@dimen/_9pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_12pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_12pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_kadang_kadang"
                    android:textAllCaps="false"
                    style="@style/btnSolidRounded_2" tools:text="@string/lbl_kadang_kadang"/>
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnSering"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_23pxh"
                    android:layout_marginStart="@dimen/_26pxh"
                    android:layout_marginTop="@dimen/_9pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_12pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_12pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_sering"
                    android:textAllCaps="false"
                    style="@style/btnSolidRounded_2" tools:text="@string/lbl_sering"/>
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnSelalu"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_25pxh"
                    android:layout_marginStart="@dimen/_26pxh"
                    android:layout_marginTop="@dimen/_9pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_12pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_12pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_selalu"
                    android:textAllCaps="false"
                    style="@style/btnSolidRounded_2" tools:text="@string/lbl_selalu"/>
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnNext"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_27pxv"
                    android:layout_marginBottom="@dimen/_23pxv"
                    android:layout_marginEnd="@dimen/_26pxh"
                    android:layout_marginStart="@dimen/_26pxh"
                    android:layout_marginTop="@dimen/_28pxv"
                    android:layout_width="@dimen/_103pxh"
                    android:paddingBottom="@dimen/_5pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_5pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_next"
                    android:textAllCaps="false"
                    style="@style/btnSolidRounded_1" tools:text="@string/lbl_next"/>
            </LinearLayout>
        </LinearLayout>
        <FrameLayout android:id="@+id/frameBottombar"
            android:layout_gravity="bottom|start"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content" style="@style/groupStylegray_900_a8cornerRadius">
            <LinearLayout android:id="@+id/linearRowhome"
                android:layout_height="match_parent"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageHome"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_18pxh"
                    android:layout_marginBottom="@dimen/_2pxv"
                    android:layout_marginStart="@dimen/_32pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_19pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_home"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_home"/>
                <ImageView android:id="@+id/imageLightbulb"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_21pxh"
                    android:layout_marginStart="@dimen/_43pxh"
                    android:layout_width="@dimen/_20pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_lightbulb"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_lightbulb"/>
                <ImageView android:id="@+id/imageSettings"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_21pxh"
                    android:layout_marginStart="@dimen/_43pxh"
                    android:layout_width="@dimen/_24pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_settings"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_settings"/>
                <ImageView android:id="@+id/imageMenu"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_17pxh"
                    android:layout_marginBottom="@dimen/_2pxv"
                    android:layout_marginStart="@dimen/_46pxh"
                    android:layout_marginTop="@dimen/_2pxv"
                    android:layout_width="@dimen/_19pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_menu"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_menu"/>
                <ImageView android:id="@+id/imageUser"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_19pxh"
                    android:layout_marginEnd="@dimen/_28pxh"
                    android:layout_marginStart="@dimen/_49pxh"
                    android:layout_marginTop="@dimen/_2pxv"
                    android:layout_width="@dimen/_18pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_user"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_user"/>
            </LinearLayout>
        </FrameLayout>
    </FrameLayout>
</layout>
